// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model riverlevel_api_key {
  no Int @default(autoincrement()) @unique
  api_key String @id
  api_secret String
  role String
  owner String
  description String?
  enable Boolean @default(true)
  issue_data DateTime @default(now())
}